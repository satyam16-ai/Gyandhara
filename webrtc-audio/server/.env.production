# Production Environment Configuration for Render

# Server Configuration
NODE_ENV=production
PORT=10000

# Logging
LOG_LEVEL=info

# Mediasoup Configuration
RTC_MIN_PORT=10000
RTC_MAX_PORT=59999

# CORS Configuration
CORS_ORIGIN=https://gyandhara-tau.vercel.app,https://gyandhara-satyam.vercel.app

# Security
HELMET_ENABLED=true
TRUST_PROXY=true

# Health Check
HEALTH_CHECK_ENABLED=true

# WebRTC Configuration
ENABLE_UDP=true
ENABLE_TCP=true
PREFER_UDP=true
PREFER_IPV4=true

# Audio Configuration
AUDIO_CODEC=opus
AUDIO_BITRATE=320000
SAMPLE_RATE=48000
CHANNELS=1

# Worker Configuration
NUM_WORKERS=auto

# Session Configuration
PING_TIMEOUT=60000
PING_INTERVAL=25000